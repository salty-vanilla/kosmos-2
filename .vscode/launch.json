{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch demo.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/demo.py",
            "args": [
                "None",
                "--task",
                "generation_obj",
                "--path",
                "/resources/models/kosmos-2-1.pt",
                "--model-overrides",
                "{'visual_pretrained': '','dict_path':'kosmos_2/data/dict.txt'}",
                "--dict-path",
                "kosmos_2/data/dict.txt",
                "--required-batch-size-multiple",
                "1",
                "--remove-bpe",
                "sentencepiece",
                "--max-len-b",
                "500",
                "--add-bos-token",
                "--beam",
                "1",
                "--buffer-size",
                "1",
                "--image-feature-length",
                "64",
                "--locate-special-token",
                "1",
                "--batch-size",
                "1",
                "--nbest",
                "1",
                "--no-repeat-ngram-size",
                "3",
                "--location-bin-size",
                "32"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}
